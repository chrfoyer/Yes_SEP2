<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="456.0" prefWidth="645.0" style="-fx-border-color: #999999; -fx-border-width: 7;"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="view.UserProfileController">
    <children>
        <HBox alignment="CENTER_RIGHT" prefHeight="88.0" prefWidth="600.0" style="-fx-background-color: #B8F0FF;">
            <children>
                <Label style="-fx-font-weight: bold;" text="Profile" textFill="#c1c1c1">
                    <font>
                        <Font size="36.0"/>
                    </font>
                    <padding>
                        <Insets right="120.0"/>
                    </padding>
                </Label>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#logout" text="Log out">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Button>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="323.0" prefWidth="631.0">
            <children>
                <VBox prefHeight="282.0" prefWidth="628.0">
                    <children>
                        <HBox prefHeight="55.0" prefWidth="586.0">
                            <children>
                                <VBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="599.0">
                                    <children>
                                        <Label fx:id="username" text="Username">
                                            <font>
                                                <Font size="14.0"/>
                                            </font>
                                            <VBox.margin>
                                                <Insets left="20.0"/>
                                            </VBox.margin>
                                        </Label>
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="581.0">
                                    <children>
                                        <HBox alignment="CENTER_RIGHT" prefHeight="55.0" prefWidth="42.0">
                                            <children>
                                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                                    <children>
                                                        <Button mnemonicParsing="false" onAction="#changePassword"
                                                                prefHeight="46.0" prefWidth="104.0"
                                                                text="Change Password" textAlignment="CENTER"
                                                                wrapText="true"/>
                                                    </children>
                                                </VBox>
                                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                                    <children>
                                                        <Button mnemonicParsing="false" onAction="#contactAdmin"
                                                                prefHeight="44.0" prefWidth="88.0" text="Contact&#10;"
                                                                textAlignment="CENTER">
                                                            <font>
                                                                <Font size="14.0"/>
                                                            </font>
                                                        </Button>
                                                    </children>
                                                </VBox>
                                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                                                    <children>
                                                        <Button mnemonicParsing="false" onAction="#payment"
                                                                prefHeight="45.0" prefWidth="97.0" text="Payment">
                                                            <font>
                                                                <Font size="14.0"/>
                                                            </font>
                                                        </Button>
                                                    </children>
                                                </VBox>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                        <HBox prefHeight="230.0" prefWidth="628.0">
                            <children>
                                <VBox prefHeight="228.0" prefWidth="420.0">
                                    <children>
                                        <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                                   minWidth="-Infinity" prefHeight="259.0" prefWidth="436.0">
                                            <children>
                                                <TableView fx:id="table" maxHeight="-Infinity" maxWidth="-Infinity"
                                                           minHeight="-Infinity" minWidth="-Infinity" prefHeight="262.0"
                                                           prefWidth="427.0">
                                                    <placeholder>
                                                        <Label text="No games rented out!"/>
                                                    </placeholder>
                                                    <columns>
                                                        <TableColumn fx:id="nameColumn" prefWidth="274.0" text="Game"/>
                                                        <TableColumn fx:id="timeColumn" prefWidth="143.0"
                                                                     text="Time left"/>
                                                    </columns>
                                                    <columnResizePolicy>
                                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                    </columnResizePolicy>
                                                </TableView>
                                            </children>
                                        </StackPane>
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER" prefHeight="228.0" prefWidth="169.0" spacing="40.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#returnGame" text="Return">
                                            <font>
                                                <Font size="18.0"/>
                                            </font>
                                        </Button>
                                        <Button mnemonicParsing="false" onAction="#extend" text="Extend">
                                            <font>
                                                <Font size="18.0"/>
                                            </font>
                                        </Button>
                                        <Button mnemonicParsing="false" onAction="#browse" text="Browse">
                                            <font>
                                                <Font size="18.0"/>
                                            </font>
                                        </Button>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="38.0" prefWidth="631.0" style="-fx-background-color: #B8F0FF;">
            <children>
                <Label fx:id="error" style="-fx-font-weight: bold;" text="ERROR" textFill="RED"/>
            </children>
        </HBox>
    </children>
</VBox>
